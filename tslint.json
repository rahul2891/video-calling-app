{
    "extends": "tslint:recommended",
    "defaultSeverity": "error",
    "rules": {
        "no-console": {
            "severity": "warning",
            "option": ["debug", "info", "log", "trace"]
        },
        "max-line-length": [true, 100],
        "semicolon": [true, "always"]
    }
       
}